<!-- DTD for the faces descriptor -->

<!ELEMENT facets (label | debug | panel | tree)*>
<!ELEMENT label (#PCDATA)>
<!ELEMENT debug (#PCDATA)>
<!ELEMENT panel (#PCDATA)>
<!ELEMENT tree (#PCDATA)>

<!-- if extends == false, will not seach in super hierarchy
        extends == name, will search in name file -->
<!ATTLIST facets
	extends CDATA #IMPLIED
>

<!ATTLIST label
	mode CDATA #IMPLIED
	default (true|false) "false"
	container (true|false) "false"
	file CDATA #IMPLIED
	extends CDATA #IMPLIED
	same CDATA #IMPLIED
	profile CDATA #IMPLIED
>

<!ATTLIST debug
	mode CDATA #IMPLIED
	default (true|false) "false"
	container (true|false) "false"
	file CDATA #IMPLIED
	extends CDATA #IMPLIED
	same CDATA #IMPLIED
	profile CDATA #IMPLIED
>

<!ATTLIST panel
	mode CDATA #IMPLIED
	default (true|false) "false"
	container (true|false) "false"
	file CDATA #IMPLIED
	extends CDATA #IMPLIED
	same CDATA #IMPLIED
	profile CDATA #IMPLIED
>

<!ATTLIST tree
	mode CDATA #IMPLIED
	default (true|false) "false"
	container (true|false) "false"
	file CDATA #IMPLIED
	extends CDATA #IMPLIED
	same CDATA #IMPLIED
	profile CDATA #IMPLIED
>
